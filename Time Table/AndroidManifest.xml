<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="blogspot.destinationengineering.TimeTable"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

<uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        
         <activity
            android:name=".intro"
                      android:theme="@android:style/Theme.NoTitleBar">
              <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>        
        
        
        
        <activity
            android:name=".TimeTableActivity"
            android:theme="@android:style/Theme.NoTitleBar" >
            <intent-filter>
                <action android:name="blogspot.destinationengineering.TimeTable.TIMETABLEACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
         <activity
            android:name=".OverView"
                        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">

            
            <intent-filter>
                <action android:name="blogspot.destinationengineering.TimeTable.OVERVIEW" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
 <activity
            android:name=".Email"
                        android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
                        >
            <intent-filter>
                <action android:name="blogspot.destinationengineering.TimeTable.EMAIL" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>         
        
        
        
        
    </application>

</manifest>